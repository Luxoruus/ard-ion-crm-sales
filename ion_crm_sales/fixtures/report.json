  "module": "Ion Crm Sales",
  "name": "Commission Expense Summary",
  "prepared_report": 0,
  "query": "SELECT je.posting_date, je.company, je.user_remark,\n       SUM(acc.debit_in_account_currency) AS expense\nFROM `tabJournal Entry Account` acc\nJOIN `tabJournal Entry` je ON acc.parent = je.name\nWHERE acc.account = %(expense_account)s\n  AND je.company = %(company)s\n  AND je.posting_date BETWEEN %(from_date)s AND %(to_date)s\nGROUP BY je.posting_date, je.company, je.user_remark\nORDER BY je.posting_date",
  "ref_doctype": "Journal Entry",
  "reference_report": null,
  "report_name": "Commission Expense Summary",
  "report_script": "filters.get(\"expense_account\") = frappe.get_single_value(\"Commission Policy Settings\", \"expense_account\")\n\n\nreturn frappe.db.get_all('Journal Entry', ['posting_date', 'company, 'user_remark'], filters = filters)",
